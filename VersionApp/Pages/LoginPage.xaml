<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VersionApp.Pages.LoginPage"
             xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"
             ios:Page.UseSafeArea="True">
    <ContentPage.Content>
        <StackLayout BackgroundColor="Beige" Orientation="Vertical" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            <Button x:Name="Button1" Margin="10" Padding="10" Text="Log in to v20.1.0" Clicked="Button_Clicked" CommandParameter="20.1.0" />
            <Button x:Name="Button2" Margin="10" Padding="10" Text="Log in to v20.1.1" Clicked="Button_Clicked" CommandParameter="20.1.1" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>